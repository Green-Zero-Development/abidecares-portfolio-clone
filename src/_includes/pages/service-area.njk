            </a>
        </div>
        <div class="w-full xl:w-4/5">
            <p class="lg-p">{{ serviceArea.servicesSection.paragraph }}</p>
            <ul class="md:flex flex-wrap list-disc pt-6 location-services-list">
                {% for item in serviceArea.servicesSection.services %}
                <li class="w-full md:w-1/2 xl:w-1/3 location-services-list-item">{{ item.title }}</li>
                {% endfor %}
            </ul>
        </div>
    </div>
</div>

{% endif %}

{% if serviceArea.awards %}

<div class="max-w-7xl mx-auto pt-12 px-6 ani-area-sing-6">
    <div class="flex flex-wrap">
        {% for item in serviceArea.awards %}
            <img src="{{ item.award.url }}" class="w-64 h-full mx-auto p-12" />
        {% endfor %}
    </div>
</div>

{% endif %}

