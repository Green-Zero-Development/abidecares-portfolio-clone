---
layout: base.njk
---

{% set regionalAdmins = [] %}
{% set areaMarketingManagers = [] %}
{% set operationsManagers = [] %}
{% set operationsCoordinators = [] %}
{% set operationsAssistants = [] %}
{% set clientCareNurses = [] %}
{% set otherRoles = [] %}
{% for item in teamMembers %}
   {% set jobTitle = item.jobTitle %}
   {% if 'Regional Administrator' in jobTitle %}
        {% set regionalAdmins = (regionalAdmins.push(item), regionalAdmins) %}
    {% elseif 'Area Marketing Manager' in jobTitle %}
        {% set areaMarketingManagers = (areaMarketingManagers.push(item), areaMarketingManagers) %}
    {% elseif 'Operations Manager' in jobTitle %}
        {% set operationsManagers = (operationsManagers.push(item), operationsManagers) %}
    {% elseif 'Operations Coordinator' in jobTitle %}
        {% set operationsCoordinators = (operationsCoordinators.push(item), operationsCoordinators) %}
    {% elseif 'Operations Assistant' in jobTitle %}
        {% set operationsAssistants = (operationsAssistants.push(item), operationsAssistants) %}
    {% elseif 'Client Care Nurse' in jobTitle %}
        {% set clientCareNurses = (clientCareNurses.push(item), clientCareNurses) %}
    {% else %}
        {% set otherRoles = (otherRoles.push(item), otherRoles) %}
   {% endif %}
{% endfor %}

<div class="bg-cover xxl:bg-contain bg-top bg-no-repeat -mt-32 pt-48 pb-12 service-hero-bk ani-area-sing-trig-1">
    <div class="max-w-8xl mx-auto pb-16 px-6 md:px-12">
        <style>
            .service-hero-img {
                background-image: url('{{ serviceArea.heroSection.image.url }}');
                border-radius: 16px;
            }
        </style>
        <div class="flex flex-wrap flex-col-reverse md:flex-row items-center">
            <div class="w-full md:w-5/12 py-12 md:py-0 md:pr-4 xl:pr-8 text-navy ani-area-sing-1">
                <div class="">
                    <h6 class="breadcrumbs"><a href="/">Home</a> / <a href="/service-areas/">Our Service Areas</a> / {{ serviceArea.city }}, {{ serviceArea.state }}</h6>
                    <h1 class="text-5xl lg:text-h3-init capitalize pt-2">{{ serviceArea.locationProperName }}</h1>
                    <h6 class="text-base pt-2 pb-4">Located in {{ serviceArea.city }}</h6>
                    <p class="text-base lg:text-lg-p-init lg:leading-normal">{{ serviceArea.heroSection.paragraph }}</p>
                    <a class="text-navy font-bold capitalize text-button"
                        {% if serviceArea.heroSection.button.link_to_where == "A page on this site" %}
                        href='{{ serviceArea.heroSection.button.onsite_link }}'
                        {% elseif serviceArea.heroSection.button.link_to_where == "Another site" %}
                        href='{{ serviceArea.heroSection.button.offsite_link }}' target='_blank'
                        {% elseif serviceArea.heroSection.button.link_to_where == "A file" %}
                                href='{{ serviceArea.heroSection.button.file_link }}' 
                        target='_blank'
                        {% endif %}
                    >
                        {{ serviceArea.heroSection.button.text }}
                    </a>
                    <div class="flex flex-wrap pt-4">
                        <div class="w-full lg:w-2/3 pb-6 lg:pb-0">
                            <svg class="mb-3" width="25" height="32" viewBox="0 0 25 32" fill="none" xmlns="http://www.w3.org/2000/svg">
