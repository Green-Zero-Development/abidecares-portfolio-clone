<script>
gsap.set(".ani-providers-1", { y: -20, opacity: 0 });
gsap.set(".ani-providers-2", { y: 20, opacity: 0 });
gsap.set(".ani-providers-3", { y: 20, opacity: 0 });

let aniProviders1 = gsap.timeline({scrollTrigger:{trigger: ".ani-providers-trig-1",start: "top +=100px"}});
aniProviders1.to(".ani-providers-1", {y: 0,opacity: 1, ease: "sine", duration: 0.5}, )
  .to(".ani-providers-2", {y: 0,opacity: 1, ease: "sine", duration: 0.5})
  .to(".ani-providers-3", {y: 0,opacity: 1, ease: "sine", duration: 0.5});

gsap.set(".ani-providers-4", { y: 30, opacity: 0 });

let aniproviders3 = gsap.timeline({scrollTrigger:{trigger: ".ani-providers-trig-2",start: "top +=1000px"}});
aniproviders3.to(".ani-providers-4", {y: 0,opacity: 1, ease: "sine", duration: 0.5}, );

gsap.set(".ani-providers-5", { y: -30, opacity: 0 });
gsap.set(".ani-providers-6", { y: 30, opacity: 0 });

let aniproviders4 = gsap.timeline({scrollTrigger:{trigger: ".ani-providers-trig-3",start: "top +=1000px"}});
aniproviders4.to(".ani-providers-5", {y: 0,opacity: 1, ease: "sine", duration: 0.5}, )
  .to(".ani-providers-6", {y: 0,opacity: 1, ease: "sine", duration: 0.5});

gsap.set(".ani-providers-7", { x: -30, opacity: 0 });
gsap.set(".ani-providers-8", { x: 30, opacity: 0 });

let aniproviders5 = gsap.timeline({scrollTrigger:{trigger: ".ani-providers-trig-4",start: "top +=1000px"}});
aniproviders5.to(".ani-providers-7", {x: 0,opacity: 1, ease: "sine", duration: 0.5}, )
  .to(".ani-providers-8", {x: 0,opacity: 1, ease: "sine", duration: 0.5});
</script>